@{
    ViewData["Title"] = "Komisyon Listesi";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
@*<head>
    <title>Add Commission</title>
    <link href='~/lib/bootstrap/dist/css/bootstrap.min.css' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="//code.jquery.com/ui/1.13.2/themes/base/jquery-ui.css">
    <script src='~/js/jquery-3.6.1.min.js' type='text/javascript'></script>
    <script src="~/js/jquery.ui.datepicker.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
</head>*@

<body>
    <div class="container">
        <div id="accordion">
            <div class="card">
                <div class="card-header" id="headingOne">
                    <h5 class="mb-0">
                        <a class="btn btn-light" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne" role="button"> Komisyon Ekle</a>
                    </h5>
                </div>

                <div style="margin-left:5%;margin-right:5%" id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
                    <br />
                    <div class="input-group mb-3">
                        <span class="input-group-text" id="inputGroup-sizing-default">Komisyon Ücreti </span>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>
                    <br />
                    <div class="input-group mb-3">
                        <span class="input-group-text" id="inputGroup-sizing-default">Minimum Fiyat </span>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>
                    <br />
                    <div class="input-group mb-3">
                        <span class="input-group-text" id="inputGroup-sizing-default">Maximum Fiyat </span>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>
                    <br />
                    <div class="input-group mb-3">
                        <span class="input-group-text" id="inputGroup-sizing-default">Başlangıç Tarihi </span>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>
                    <br />
                    <div class="input-group mb-3">
                        <span class="input-group-text" id="inputGroup-sizing-default">Bitiş Tarihi </span>
                        <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                    </div>
                    <button class="btn btn-primary" style="float:right; margin-top:1%;margin-bottom:3%;" type="submit"><i class="fa-regular fa-floppy-disk"></i> Kaydet</button>
                </div>
            </div>
        </div>
        <br />
        <br />
        <div id="accordion">
            <div class="card">
                <div class="card-header" id="headingTwo">
                    <h5 class="mb-0">                       
                        <a class="btn btn-light" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo" role="button"> Komisyon Listesi</a>
                    </h5>
                </div>
                <div style="margin-left:5%;margin-right:5%" id="collapseTwo" class="collapse show" aria-labelledby="headingTwo" data-parent="#accordion">
                    <br />
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th scope="col">Komisyon Numarası</th>
                                <th scope="col">Komisyon Tutarı</th>
                                <th scope="col">Komisyon Başlangıç Tarihi</th>
                                <th scope="col">Komisyon Bitiş Tarihi</th>
                                <th scope="col">Komisyon Minimum Tutarı</th>
                                <th scope="col">Komisyon Maximum Tutarı</th>
                                <th scope="col">Güncelle</th>
                                <th scope="col">Sil</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>Hede</td>
                                <td>Bidi</td>
                                <td>Büdü</td>
                                <td>Hidi</td>
                                <td>Hüdü</td>
                                <td>
                                    <a class="btn btn-warning" data-toggle="modal" data-target="#edit" href="#"> <i class="fa-solid fa-pen-to-square" style="color:black;"></i> Güncelle</a>
                                </td>
                                <td>
                                    <a class="btn btn-danger" data-toggle="modal" data-target="#delete" href="#"><i class="far fa-trash-alt"></i> Sil</a>
                                </td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>Hede</td>
                                <td>Bidi</td>
                                <td>Büdü</td>
                                <td>Hidi</td>
                                <td>Hüdü</td>
                                <td>
                                    <a class="btn btn-warning" data-toggle="modal" data-target="#edit" href="#"> <i class="fa-solid fa-pen-to-square" style="color:black;"></i> Güncelle</a>
                                </td>
                                <td>
                                    <a class="btn btn-danger" data-toggle="modal" data-target="#delete" href="#"><i class="far fa-trash-alt"></i> Sil</a>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="modal" id="delete" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel1" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Trameri Sil</h5>
                    <button class="btn btn-dark btn-circle btn-sm" type="button" data-bs-dismiss="modal" aria-label="Close">
                        <i class="fa-solid fa-x"></i>
                    </button>
                </div>
                <div class="modal-body">Silmek istediğinize emin misiniz?</div>
                <div class="modal-footer"><button class="btn btn-secondary" type="button" data-bs-dismiss="modal">Kapat</button><button class="btn btn-danger" type="button">Sil</button></div>
            </div>
        </div>
    </div>
    <div class="modal" id="deleteCmp" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel3" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Tramer Bileşenini Sil</h5>
                    <button class="btn btn-dark btn-circle btn-sm" type="button" data-bs-dismiss="modal" aria-label="Close">
                        <i class="fa-solid fa-x"></i>
                    </button>
                </div>
                <div class="modal-body">Silmek istediğinize emin misiniz?</div>
                <div class="modal-footer"><button class="btn btn-secondary" type="button" data-bs-dismiss="modal">Kapat</button><button class="btn btn-danger" type="button">Sil</button></div>
            </div>
        </div>
    </div>
</body>
</html>
