<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title></title>
    <link href="https://fonts.googleapis.com/css?family=Arial&display=swap" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Jockey+One&display=swap" rel="stylesheet" />
    <link href="~/Content/MyCss/SignUp/CustomerSignUp/CustomerSignUp.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap.js"></script>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
</head>

<body>

    <div class="SignUp">
        <div class="SignUpInputs">
            <div class="Inputs">
                <span class="SignUpTextOne">Bireysel Kullanıcı Oluştur</span>
                <span class="SignUpTextTwo">Ikinci El Araç Ihale Sistemi Kayıt Olun.</span>
                <input class="SignUpUsername" type="text" placeholder="Kullanıcı Adı:" />
                <input class="SignUpName" type="text" placeholder="Ad Soyad:" />
                <input class="SignUpPhone" type="text" placeholder="Telefon:" />
                <input class="SignUpEmail" type="email" placeholder="Email:" />
                <input class="SignUpPassword" type="password" placeholder="Şifre" />
                <input class="SignUpCheckPassword" type="password" placeholder="Şifre Tekrarı" />
                <div class="Grp">
                    <a href="~/Auth/Login" class="SignUpJoinUsTwo">Geri Dön</a>
                    <input class="SignUpJoinUsOne" type="button" value="Kayıt Ol" />
                </div>
            </div>
        </div>
        @RenderBody()
    </div>
</body>
</html>